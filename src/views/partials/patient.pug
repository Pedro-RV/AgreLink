mixin patient(patient)
    <div class="col s12 m6 l4 xl3">
        <div class="card">
            <div class="card-image">
                <img src="/images/default-profile.png">
            </div>
            <div class="card-content">
                <span class="card-title center">#{patient.name}</span>
            </div>
        </div>
    </div>